{
  "name": "ferrow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:workers": "tsx src/lib/jobs/start-workers.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:workers\"",
    "build": "echo 'Starting build...' && NEXT_PHASE=phase-production-build next build",
    "start": "next start",
    "start:workers": "tsx src/lib/jobs/start-workers.ts",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
